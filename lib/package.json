{
  "name": "ennabl-ui-kit-beta",
  "version": "0.3.1",
  "type": "module",
  "description": "A React component library with reusable UI components for document management and insurance applications",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "BEST_PRACTICES.md",
    "QUICK_REFERENCE.md",
    "RELEASE_WORKFLOW.md"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "update-metadata": "node scripts/update-metadata.js",
    "prepublishOnly": "npm run update-metadata && npm run build && npm run type-check",
    "track-components": "node scripts/track-components.js",
    "check-components": "node scripts/check-components.js",
    "check-status": "node scripts/check-components.js status",
    "check-recent": "node scripts/check-components.js recent",
    "check-new": "node scripts/check-components.js new",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx",
    "test-build": "npm run build && node -e \"console.log('✅ Build successful')\"",
    "verify-exports": "node scripts/verify-exports.js",
    "pre-release": "npm run track-components && npm run check-status && npm run type-check && npm run build",
    "dry-run": "npm pack",
    "test-install": "npm run dry-run && mkdir -p ../test-install && cd ../test-install && npm init -y && npm install ../lib/ennabl-ui-kit-beta-*.tgz && node -e \"const { Header, Footer } = require('ennabl-ui-kit-beta'); console.log('✅ Components imported successfully');\"",
    "release-checklist": "node scripts/release-checklist.js"
  },
  "keywords": [
    "react",
    "typescript",
    "ui-components",
    "design-system",
    "document-management",
    "insurance",
    "mock-data",
    "faker"
  ],
  "author": "Your Name",
  "license": "MIT",
  "peerDependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-tooltip": "^1.1.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.462.0",
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "postcss": "^8.4.31",
    "postcss-import": "^15.1.0",
    "rollup": "^4.9.6",
    "rollup-plugin-dts": "^6.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.11",
    "@faker-js/faker": "^9.9.0"
  }
}
